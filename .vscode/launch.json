    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'tikv-service-server'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=tikv-service-server",
                    "--package=tikv-service"
                ],
                "filter": {
                    "name": "tikv-service-server",
                    "kind": "bin"
                }
            },
            "args": [
                "--port=6666",
                "--pdaddrs=192.168.31.6:2379",
                "--config=config.toml"
            ],
            "cwd": "${workspaceFolder}"
        }
    ]
}